#: league/templates/league/event.html:27
msgid "The <b>%(name)s</b> started on <b>%(begin_time)s</b> (%(TIME_ZONE)s) and "
msgstr "El <b>%(name)s</b> comenzado el <b>%(begin_time)s</b> (%(TIME_ZONE)s) y "

#: league/templates/league/event.html:30
msgid "%(number_players)s players played %(number_games)s games in %(number_divisions)s divisions. That is %(percent_game_played)s %% of %(possible_games)s possible games."
msgstr "%(number_players)s personas jugaron %(number_games)s partidas en %(number_divisions)s divisiones. Eso es %(percent_game_played)s %% de %(possible_games)s partidas posibles."

#: league/templates/league/event.html:37
msgid "Games must use <b>Japanese timesetting</b> with at least <b>%(get_main_time_min)s min main time</b> and <b>3 x %(additional_time)s sec byo-yomi</b>."
msgstr "Las partidas deben usar <b>byo-yomi japonés</b> con al menos <b>%(get_main_time_min)s min de tiempo principal</b> y <b>3 x %(additional_time)s seg byo-yomi</b>."

#: league/templates/league/event.html:38
msgid "At least one player must tag the game saying <b>%(tag_upper)s</b> or <b>%(tag_lower)s</b>."
msgstr "Por lo menos un jugador debe etiqutar la partida comentando <b>%(tag_upper)s</b> o <b>%(tag_lower)s</b> en el chat."

#: league/templates/league/event.html:40
msgid "Komi must be set to <b>%(komi)s</b>."
msgstr "El Komi debe ser de <b>%(komi)s</b>."

#: league/templates/league/event.html:46
msgid "Players can play up to <b>%(nb_matchs)s games</b> against the same opponent "
msgstr "Jugadores pueden jugar hasta <b>%(nb_matchs)s partidas</b> contra el mismo oponente "

#: league/templates/league/event.html:47
msgid "A win grants <b>%(ppwin)s points</b> and a loss grants <b>%(pploss)s</b>."
msgstr "Una victoria otorga <b>%(ppwin)s puntos</b> y una derrota otorga <b>%(pploss)s</b>."

#: league/templates/league/event.html:48
msgid "Players who played at least <b>%(min_matchs)s games</b> will be added to the next league, once this one has ended."
msgstr "Jugadores que hayan jugado al menos <b>%(min_matchs)s partidas</b> serán agregados a la siguiente liga, una vez que ésta haya finalizado."

